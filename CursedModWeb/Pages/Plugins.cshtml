@page
@model CursedModWeb.Pages.Plugins
@{
    ViewData["Title"] = "Plugins";
}

<div class="title">
    <div class="nav-content">
        <div style="margin-bottom: 1rem">
            <h1>Plugins</h1>
            <span style="margin-bottom: 5px">Explore all the official community-made plugins!</span>
        </div>
        <div class="sb">
            <svg class="search-icon" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 16 16" width="16" height="16"><path fill-rule="evenodd" d="M11.5 7a4.499 4.499 0 11-8.998 0A4.499 4.499 0 0111.5 7zm-.82 4.74a6 6 0 111.06-1.06l3.04 3.04a.75.75 0 11-1.06 1.06l-3.04-3.04z"></path></svg>
            <input class="search" id="search" type="text" placeholder="Type here to search">
        </div>
    </div>
</div>

<div class="section">
    <div class="market" id="market">
        @for (int i = 0; i < 40; i++)
        {
            <div style="width: 323px; height: 190px; background-color: var(--secondary-button); border-radius: 10px"> </div>
        }
    </div>
</div>


@section Scripts
{
    <script src="~/js/site.js" asp-append-version="true"></script>
}
